{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9]+$": {
      "type": "object",
      "properties": {
        "aboutme": {
          "type": "string"
        },
        "gender": {
          "type": "string"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "location": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "aboutme", 
        "gender", 
        "image", 
        "location",
        "name"
      ],
      "additionalProperties": false
    }
  }
}